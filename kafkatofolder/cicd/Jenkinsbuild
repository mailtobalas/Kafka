pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                sh 'echo checkout'
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/mailtobalas/Kafka.git']]])
            }
        }

    }
    post {
        always {
            echo 'Cleanup after everything!'
        }
    }
}